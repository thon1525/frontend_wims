{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://wims-z0uz.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://frontendwimssystem-jtin7gouy-horn-thorns-projects.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "content-type,authorization,cookie,x-csrftoken,accept,accept-encoding,origin,user-agent"
        },
        {
          "key": "Access-Control-Expose-Headers",
          "value": "set-cookie,content-type,authorization,x-csrftoken"
        }
      ]
    }
  ]
}